config dnsmasq
	option domainneeded	0
	option boguspriv	0
	option filterwin2k	0
	option localise_queries	0
	option domain		'ptp'
	option expandhosts	0
	option nonegcache	0
	option authoritative	1
	option readethers	1
	option nohosts		1
	option rebind_protection	0
	option leasefile	'/tmp/dhcp.leases'
	option resolvfile	'/tmp/resolv.conf.auto'
	list server		'/ptp/70.102.34.162'
	list server		'/11.10.in-addr.arpa/70.102.34.162'

config dhcp private
	option interface	private
	option start		100
	option limit		150
	option leasetime	12h

config dhcp public
	option interface	public
	option start		PTP_DHCPSTART_PTP
	option limit		1024
	option leasetime	1h

config dhcp wan
	option interface	wan
	option ignore		1
