config 'interface' 'loopback'
	option 'ifname' 'lo'
	option 'proto' 'static'
	option 'ipaddr' '127.0.0.1'
	option 'netmask' '255.0.0.0'

config 'interface' 'public'
	option 'ifname' 'PTP_PUBIFACE_PTP'
	option 'proto' 'static'
	option 'ipaddr' 'PTP_PUBADDR_PTP'
	option 'netmask' 'PTP_PUBNETMASK_PTP'
        option 'ip6addr' 'PTP_PUB6PREFIX_PTP1/64'

config 'interface' 'wan'
	option 'ifname' 'PTP_WANIFACE_PTP'
	option 'proto' 'dhcp'
	#option 'proto' 'static'
	#option 'ipaddr' '10.1.10.2'
	#option 'netmask' '255.255.255.0'
	#option 'gateway' '10.1.10.1'
	#option 'dns' '10.1.10.1'
